{
  "manifest_version": 2,
 
  "name": "Tinder",
  "description": "Tinder Login",
  "version": "1.0",
   "permissions": [
    "tabs", "<all_urls>"
  ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery.js"
            ],
            "run_at": "document_end"
        }
    ], 
  "browser_action": {
    "default_popup": "popup.html"
  }
}